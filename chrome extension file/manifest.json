

{
    "name": "ChatGPT Plus",
    "version": "1.3",
    "description": "Paste powerful prompt in chatGPT with easy step",
    "manifest_version": 3,
    "icons": {
      "32": "./Icon32.png",
      "128": "./Icon128.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab",
    "scripting"]
  }