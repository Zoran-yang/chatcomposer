

{
    "name": "ChatGPT-Composer",
    "version": "1.0",
    "description": "paste prompt in chatGPT with easy step",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab",
    "tabs","scripting"]
  }